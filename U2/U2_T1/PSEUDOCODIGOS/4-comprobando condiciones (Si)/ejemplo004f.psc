Algoritmo ejemplo004f
	
	Escribir "Dime un numero"
	Leer primerNumero
	
	Escribir "Dime otro numero"
	Leer segundoNumero
	
	Escribir "Dime otro numero"
	Leer tercerNumero
	
	
	Si primerNumero > segundoNumero Entonces
		Escribir "El primero es mayor"
	SiNo
		
	Fin Si
	Si primerNumero > tercerNumero Entonces
		Escribir "El primero es mayor"
	SiNo
		
	Fin Si
	Si segundoNumero > tercerNumero Entonces
		Escribir "El segundo es mayor"
	SiNo
		Escribir "El tercero es mayor"
	Fin Si
	
FinAlgoritmo
